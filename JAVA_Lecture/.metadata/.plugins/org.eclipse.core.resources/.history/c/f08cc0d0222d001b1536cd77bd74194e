package p1;
//추상클래스 : 일반클래스와 똑같으나, 추상메소드를 가질 수 있다. 
//추상 클래스 = 일반 클래스 + 추상메소드
public class Phone {
	String owner;
	String telphone;
	int price;
	
	public Phone() {
		System.out.println("기본생성자");
	}
	public Phone(String owner, String telphone, int price) {
		this.owner = owner;
		this.telphone = telphone;
		this.price = price;
		System.out.println("명시적 생성자");
	}
	
}
